@{

    Layout = null;

    string UserName = "";

    string imgUrl = "~/Assets/Custom/Images/AnonimImg.jpg";
    int id = 0;
    int BtPrice = 0;

    if (((chat.UTILITIES.SessionOperations.EmpCusSessionContext)Session["EmpCusSessionContext"]) != null)
    {
        UserName = ((chat.UTILITIES.SessionOperations.EmpCusSessionContext)Session["EmpCusSessionContext"]).UserName;
        BtPrice = ((chat.UTILITIES.SessionOperations.EmpCusSessionContext)Session["EmpCusSessionContext"]).BtPrice;
        id = ((chat.UTILITIES.SessionOperations.EmpCusSessionContext)Session["EmpCusSessionContext"]).ID;
        imgUrl = ((chat.UTILITIES.SessionOperations.EmpCusSessionContext)Session["EmpCusSessionContext"]).ImageUrl;
    }
    else
    {
        //Yönlenme
    }

    if (BtPrice < 0)
    {
        BtPrice = 0;
    }

}

